#include <stdio.h>
int main() {
    int n;
    int sum = 0;
    printf("Enter the size of the array: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }
    printf("The sum of all elements in the array is: %d\n", sum);
    return 0;
}
